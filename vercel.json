    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "images": {
    "domains": [],
    "sizes": [16, 32, 48, 64, 96, 128, 256, 384, 512, 1024, 2048],
    "remotePatterns": [],
    "minimumCacheTTL": 60
  }
}
